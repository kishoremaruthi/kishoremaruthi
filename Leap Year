import java.io.*;
import java.util.Scanner;
public class TestClass {
	 public static void main(String[] args) { 
		int a;
       Scanner sc=new Scanner(System.in);
       a=sc.nextInt();
       if(a%4==0)
       {
         System.out.println(a+" is a leap year");
       }
       else
       {
         System.out.println(a+" is not a leap year");
       }
	}
}
