import java.io.*;
import java.util.Scanner;
public class TestClass {
	 public static void main(String[] args) { 
		int num;
     
       Scanner sc=new Scanner(System.in);
        num=sc.nextInt();
        int sum=0;
        for(int i=0;i<num;i++)
            sum+=sc.nextInt();
        System.out.println(sum);
 
    }
}
