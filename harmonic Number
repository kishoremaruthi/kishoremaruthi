import java.io.*;
import java.util.Scanner;
public class TestClass {
	 public static void main(String[] args) { 
		float a,i;
        float sum=0;
       Scanner sc=new Scanner(System.in);
       a=sc.nextInt();
       for ( i=1;i<=a;i++)
       {
         sum=sum+(1/i);
       }
       System.out.print(String.format("%.2f",sum));
	}
}
