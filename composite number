import java.io.*;
import java.util.Scanner;
public class TestClass {
	 public static void main(String[] args) { 
       int a;
       Scanner sc=new Scanner(System.in);
       a=sc.nextInt();
       
		int count=0;
	
for(int i=1; i<=a; i++)
  {
   if(a% i == 0)
    count++;
  }
  
  if(count > 2)
   System.out.format("yes");  
  else
   System.out.format("no");
 }
}
